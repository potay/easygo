{
  "background": {
    "scripts": [ "background.js" ]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "description": "Redirects stuff using webRequest",
  "manifest_version": 2,
  "minimum_chrome_version": "17",
  "name": "easygo",
  "permissions": [ 
    "storage",
    "webRequest", 
    "webRequestBlocking",
    "http://potay/*" 
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "0.1.1"
}
